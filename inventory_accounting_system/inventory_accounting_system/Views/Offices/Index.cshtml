@model IEnumerable<Asset>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Офисы</h2>
<p>
    <a class="btn btn-primary" href="/Account/Register">Добавить сотрудника</a>
</p>

<form>
    <div class="form-group" id="catSelect">
        <select name="officeId" class="target form-control" id="onChangeText" asp-items="ViewBag.Offices"></select>
    </div>
    <button id="officeButton" class="btn btn-primary" style="margin-bottom: 20px">Get</button>
</form>
<p>
    <a class="btn btn-primary" asp-action="Create">Создать офис</a>
</p>
@{
    if (Model != null)
    {
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Category</th>
                    <th scope="col">Prefix</th>
                    <th scope="col">Employee</th>
                    <th scope="col">Handle</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Category.Name</th>
                        <td>@item.InventPrefix</td>
                        <td><a asp-controller="Employes" asp-action="UserAssets" asp-route-id="@item.Employee.Id">@item.Employee.Name</a></td>
                        <td></td>
                    </tr>
                }
            </tbody>
        </table>
    }
}
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#officeButton').click(function(e) {
            e.preventDefault();
            var val = document.getElementById("onChangeText").value

            $.ajax({
                type: 'GET',
                url: '@Url.Action("Index", "Employes")',
                data: { officeId: val },
                dataType: 'json',
                success: console.log('ok')
            }
        });
    })
</script>